{
	"name": "giphy-trending",
	"private": true,
	"version": "0.0.0",
	"description": "giphy trending search using React and Typescript",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"test": "jest",
		"preview": "vite preview"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ngocsangyem/giphy-trending.git"
	},
	"keywords": [
